{
  "manifest_version": 3,
  "name": "LinkedIn Ai Assistant",
  "version": "1.0",
  "description": "Assists users in generating replies to LinkedIn messages.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://www.linkedin.com/*"],
      "js": ["contentScript.js"],
      "css": ["styles/index.css"]
    }
  ],
  "icons": {
    "16": "icons/ai-icon.svg",
    "48": "icons/ai-icon.svg",
    "128": "icons/ai-icon.svg"
  }
}
